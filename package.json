    "execa": "0.10.0",
    "jest": "23.4.2"
  },
  "peerDependencies": {
    "execa": "^0.10.0"
  },
  "jest": {
    "coverageDirectory": ".jest-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
